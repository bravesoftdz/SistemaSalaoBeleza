unit UntTelaCadastroClientes;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, UntTelaBase01, Vcl.StdCtrls,
  AeroButtons, W7Classes, W7Panels, AdvEdit, AdvEdBtn, JvToolEdit, JvExStdCtrls,
  JvEdit, JvValidateEdit, Vcl.Mask, JvExMask, JvMaskEdit, JvCheckedMaskEdit,
  JvDatePickerEdit, JvExComCtrls, JvDateTimePicker, Vcl.ComCtrls,
  AdvDateTimePicker;

type
  TFrmTelaCadastroClientes = class(TFrmTelaBase01)
    AdvEditBtn1: TAdvEditBtn;
    AdvEditBtn2: TAdvEditBtn;
    AdvEditBtn3: TAdvEditBtn;
    AdvEditBtn4: TAdvEditBtn;
    AdvEditBtn5: TAdvEditBtn;
    AdvDateTimePicker1: TAdvDateTimePicker;
    AdvEdit1: TAdvEdit;
    AdvEdit2: TAdvEdit;
  private
    { Private declarations }
  public
    { Public declarations }

    class procedure AbrirTelaCadastroClientes;
  end;



implementation

{$R *.dfm}

{ TFrmTelaCadastroClientes }



class procedure TFrmTelaCadastroClientes.AbrirTelaCadastroClientes;
var
  FrmTelaCadastroClientes: TFrmTelaCadastroClientes;
begin
    FrmTelaCadastroClientes:= TFrmTelaCadastroClientes.Create(nil);
    try
      FrmTelaCadastroClientes.ShowModal

    finally
      FreeAndNil(FrmTelaCadastroClientes);
    end;
end;

end.
