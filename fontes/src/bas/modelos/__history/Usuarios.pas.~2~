unit Usuarios;

interface

type
  TUsuarios = class
  private
    FID: Integer;
    FNome: string;
    FLogin: string;
    FSenha: string;

  published
    property ID: Integer read FID write FID;
    property Nome: string read FNome write FNome;

    property Login: string read FLogin write FLogin;
    property Senha: string read FSenha write FSenha;
    

  end;


implementation

end.
