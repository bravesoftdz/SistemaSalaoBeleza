unit Profissioes;

interface

type
  TProfissoes = class
  private
    FID: Integer;
    FDescricao: string;

  published
    [TMapaDB('', '',  )]
    property ID: Integer read FID write FID;
    [TMapaDB('', '',  )]
    property Descricao: string read FDescricao write FDescricao;

  end;

implementation

end.
