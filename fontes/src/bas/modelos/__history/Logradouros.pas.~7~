unit Logradouros;

interface

uses
  Modelo,
  MapaDB;

type
  TLogradouros = class
  private
    FID: Integer;
    FDescricao: string;

  published
    [TMapaDB('logradouros', 'id_logradouro', atrChavePrimaria )]
    property ID: Integer read FID write FID;

    [TMapaDB('logradouros', 'ds_logradouro', atrNenhum )]
    property Descricao: string read FDescricao write FDescricao;


  end;

implementation

end.
