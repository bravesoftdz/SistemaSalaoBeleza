unit UF;

interface

uses
  Modelo,
  MapaDB;

type
  TUF = class
  private
    FID: Integer;
    FUF: string;
    FDescricao: string;
  published
    [TMapaDB('ufs', 'id_uf', [atrPesquisa] )]
    property ID: Integer read FID write FID;

    [TMapaDB('ufs', 'cd_uf', [] )]
    property UF: string read FUF write FUF;

    [TMapaDB('ufs', 'ds_uf', [] )]
    property Descricao: string read FDescricao write FDescricao;

  end;

implementation

end.
