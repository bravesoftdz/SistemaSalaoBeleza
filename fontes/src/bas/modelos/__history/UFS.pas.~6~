unit UFS;

interface

uses
  Modelo,
  MapaDB;

type
  TUFS = class
  private
    FID: Integer;
    FUF: string;
    FDescricao: string;
  published
    [TMapaDB('ufs', 'id_uf', atrChavePrimaria )]
    property ID: Integer read FID write FID;

    [TMapaDB('ufs', 'cd_uf', atrNenhum )]
    property UF: string read FUF write FUF;

    [TMapaDB('ufs', 'ds_uf', atrNenhum )]
    property Descricao: string read FDescricao write FDescricao;

  end;

implementation

end.
