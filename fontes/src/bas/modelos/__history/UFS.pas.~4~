unit UFS;

interface

type
  TUFS = class
  private
    FID: Integer;
    FUF: string;
    FDescricao: string;
  published
    [TMapaDB('', '',  )]
    property ID: Integer read FID write FID;
    [TMapaDB('', '',  )]
    property UF: string read FUF write FUF;
    [TMapaDB('', '',  )]
    property Descricao: string read FDescricao write FDescricao;

  end;

implementation

end.
