unit Bairros;

interface

uses
  Modelo,
  MapaDB;

type
  TBairros = class(TModelo)
  private
    FID: Integer;
    FDescricao: string;

  published
    [TMapaDB('bairros', 'id_bairro', atrChavePrimaria )]
    property ID: Integer read FID write FID;

    [TMapaDB('bairros', 'ds_bairro', atrNenhum )]
    property Descricao: string read FDescricao write FDescricao;
  end;

implementation

end.
