unit Bairros;

interface

type
  TBairros = class
  private
    FID: Integer;
    FDescricao: string;

  published
    [TMapaDB('bairros', 'id_bairro',  )]
    property ID: Integer read FID write FID;

    [TMapaDB('bairros', 'ds_bairro',  )]
    property Descricao: string read FDescricao write FDescricao;

  end;

implementation

end.
