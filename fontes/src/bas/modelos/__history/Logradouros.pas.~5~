unit Logradouros;

interface

type
  TLogradouros = class
  private
    FID: Integer;
    FDescricao: string;

  published
    [TMapaDB('logradouros', 'id_logradouro',  )]
    property ID: Integer read FID write FID;

    [TMapaDB('logradouros', 'ds_logradouro',  )]
    property Descricao: string read FDescricao write FDescricao;


  end;

implementation

end.
