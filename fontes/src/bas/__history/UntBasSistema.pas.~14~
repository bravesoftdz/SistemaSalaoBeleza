unit UntBasSistema;

interface

type
  TDimencoesAberturaTela = record
    DistanciaTopo: Integer;
    DistanciaEsquerda: Integer;
    LarguraPadrao: Integer;
    AlturaPadrao: Integer;
  end;

  TSistema = class
  public
    DimencoesAberturaTela: TDimencoesAberturaTela;
  end;

var
  Sistema: TSistema;

implementation


initialization

  Sistema:= TSistema.Create;


end.
